class Solution:
    def rotateString(self, s: str, goal: str) -> bool:
        temp=s+s
        if goal in temp:
            return True
        return False
        # return len(s) == len(goal) and goal in (s + s)

#bruteforce
        if len(s)!=len(goal):
            return False
        for i range(len(s)):
            s=s[1:]+s[0]
            if s==goal:
                return True
        return False

        
