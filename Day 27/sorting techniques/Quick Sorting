class Solution:
    def quickSort(self, nums):
        n=len(nums)
        if n<=1:
            return nums
        pivot=nums[-1] # choose last element as pivot
        left=[]
        right=[]
        for i in range(n-1):
            if nums[i]<=pivot:
                left.append(nums[i])
            else:
                right.append(nums[i])
        return self.quickSort(left)+[pivot]+self.quickSort(right)



