class Solution:
    def singleNumber(self, nums: List[int]) -> int:
        result=0
        for num in nums:
            result^=num
        return result


#better
        # freq={}
        for num in nums:
            freq[num]=freq.get(num,0)+1
        for key in freq: 
            if freq[key]==1:
                return key
                
#bruteforce
        n=len(nums)
        for i in range(n):
            count=0
            for j in range(n):
                if nums[i]==nums[j]:
                    count+=1

            if count==1:
                return nums[i]
