class Solution:
    def divisors(self, n):
         # To store the divisors
        ans = []
        
        sqrtN = int(math.sqrt(n))
        
        # Iterate from 1 to sqrtN
        for i in range(1, sqrtN + 1):
            
            # If a divisor is found
            if n % i == 0:
                # Add it to the answer
                ans.append(i)
                
                # Add the counterpart divisor
                # if it's different from i
                if i != n // i:
                    ans.append(n // i)
        
        # Sorting the result 
        ans.sort()
        
        # Return the result
        return ans
                

