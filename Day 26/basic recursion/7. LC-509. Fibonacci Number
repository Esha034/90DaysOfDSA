class Solution:
    def fib(self, n: int) -> int:
#optimal
        if n == 0:
            return 0
        if n == 1:
            return 1
        
        a, b = 0, 1   # F(0), F(1)
        
        for _ in range(2, n + 1):
            a, b = b, a + b
        
        return b

#bruteforce
        # if n==0:
        #     return 0
        # if n==1:
        #     return 1
        # return self.fib(n-1)+self.fib(n-2)

