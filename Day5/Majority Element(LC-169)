class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        count=0
        res=0
        for num in nums:
            if count==0:
                res=num
            count+=(1 if num==res else -1)
        return res



#better
        n=len(nums)
        freq={}
        for num in nums:
            freq[num]=freq.get(num,0)+1

            if freq[num]>n/2:
                return num


#bruteforce
        n=len(nums)
        for i in range(n):
            count=0
            for j in range(n):
                if nums[i]==nums[j]:
                    count+=1
            if count>n//2:
                return nums[i]

            
