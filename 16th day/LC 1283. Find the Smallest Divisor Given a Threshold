class Solution:
    def smallestDivisor(self, nums: List[int], threshold: int) -> int:
        n=len(nums)
        maxnum=nums[0]
        for i in range(1,n):
            if nums[i]>maxnum:
                maxnum=nums[i]

        low=1
        high=maxnum
        res=high
        while low<=high:
            mid=(low+high)//2

            sum=0
            for j in range(n):
                sum+=(nums[j]+mid-1)//mid
            if sum<=threshold:
                res=mid
                high=mid-1
            else:
                low=mid+1
        return res
        


            
        
        
